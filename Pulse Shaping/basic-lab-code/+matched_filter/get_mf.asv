function [mf_pulse] = get_mf(g_pulse)
%GET_MF 


mf_pulse = zeros(size(g_pulse));


% m_response = conv(mf_pulse, g_pulse); % mf_pulse * g_pulse = K.(g_pulse * g_pulse)

a = conv(g_pulse, g_pulse); 
peak = max(a);  

K = 1/peak; % the convolution of mf_pulse and g_pulse should have the value 1 at the sampling point meaninf the peak.

mf_pulse = K * g_pulse;
end

